# PDF Extractor CLI Configuration

# OCR Settings
PDF_EXTRACTOR_OCR_DPI=300
PDF_EXTRACTOR_OCR_USE_GPU=false
PDF_EXTRACTOR_OCR_CONFIDENCE_THRESHOLD=0.3

# Cleaning
PDF_EXTRACTOR_DEFAULT_SYSTEM=auto
# Comma-separated list of terms to remove
PDF_EXTRACTOR_CUSTOM_BLACKLIST=

# Batch Processing
PDF_EXTRACTOR_BATCH_MAX_WORKERS=2
PDF_EXTRACTOR_BATCH_TIMEOUT_SECONDS=600

# Logging
PDF_EXTRACTOR_LOG_LEVEL=INFO
# Leave empty for console-only logging
PDF_EXTRACTOR_LOG_FILE=

# Export
PDF_EXTRACTOR_DEFAULT_EXPORT_FORMAT=txt
