# Trello MCP API - Local Development
# Copy to .env for local development

# Required: Trello API Credentials
# Get from: https://trello.com/power-ups/admin
TRELLO_API_KEY=your_trello_api_key_here
TRELLO_API_TOKEN=your_trello_token_here

# Optional: Rate Limiting
# Conservative default: 90 req/10s (Trello allows 100)
RATE_LIMIT_PER_10_SECONDS=90

# Optional: Logging
LOG_LEVEL=INFO
