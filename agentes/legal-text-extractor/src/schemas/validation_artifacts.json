{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "System Validation Artifacts Schema",
  "version": "1.0.0",
  "description": "Artefatos de validacao por sistema processual brasileiro. ESTE SCHEMA NAO DEVE SER USADO EM CLASSIFICACAO SEMANTICA - serve apenas para remocao de ruido em step_01/step_02.",
  "_warning": "NUNCA importe este schema em step_03_extract.py ou step_04_classify.py",
  "metadata": {
    "created": "2025-11-25",
    "purpose": "Separacao arquitetural system-aware vs system-agnostic",
    "architecture_doc": "docs/ARCHITECTURE_PRINCIPLES.md"
  },
  "systems": {
    "PJE": {
      "name": "Processo Judicial Eletronico",
      "provider": "CNJ",
      "artifacts": [
        {
          "id": "pje_verification_code",
          "description": "Codigo de verificacao PJE (formato XXXX.9999.9XX9.X9XX)",
          "regex": "c[óo]digo\\s+de\\s+verifica[çc][ãa]o:\\s*[A-Z0-9]{4}\\.[0-9]{4}\\.[0-9A-Z]{4}\\.[A-Z0-9]{4}",
          "example": "Código de verificação: ABCD.1234.5EF6.G7HI",
          "location": "footer"
        },
        {
          "id": "pje_timestamp",
          "description": "Timestamp de geracao PJE",
          "regex": "este\\s+documento\\s+foi\\s+gerado\\s+pelo\\s+usu[áa]rio\\s+\\d{3}\\.\\d{3}\\.\\d{3}-\\d{2}\\s+em\\s+\\d{2}/\\d{2}/\\d{4}\\s+\\d{2}:\\d{2}:\\d{2}",
          "example": "Este documento foi gerado pelo usuário 123.456.789-00 em 25/11/2025 14:30:00",
          "location": "footer"
        },
        {
          "id": "pje_validation_url",
          "description": "URL de validacao PJE",
          "regex": "https?://[a-z0-9.-]*\\.(trt|trf|tst|cnj)\\d*\\.jus\\.br/[^\\s]*validar[^\\s]*",
          "example": "https://pje.trt2.jus.br/validar/ABCD1234",
          "location": "footer"
        },
        {
          "id": "pje_dual_signature",
          "description": "Tarja de assinatura dupla PJE (Resolucao CNJ 281/2019)",
          "regex": "documento\\s+assinado\\s+por\\s+[^\\n]{5,100}\\s+e\\s+certificado\\s+digitalmente\\s+por\\s+[^\\n]{5,100}",
          "example": "Documento assinado por JOAO SILVA e certificado digitalmente por AC VALID",
          "location": "footer"
        },
        {
          "id": "pje_qr_placeholder",
          "description": "QR Code placeholder PJE",
          "regex": "\\[QR\\s+CODE\\]|\\{QR\\s+CODE\\}",
          "example": "[QR CODE]",
          "location": "footer"
        },
        {
          "id": "pje_footer_bar",
          "description": "Rodape PJE generico",
          "regex": "^[_\\-=]+\\s*processo\\s+judicial\\s+eletr[ôo]nico\\s*[_\\-=]+$",
          "example": "__________________ Processo Judicial Eletrônico __________________",
          "location": "footer"
        },
        {
          "id": "pje_event_codes",
          "description": "Codigos internos de eventos PJE (NAO USAR PARA CLASSIFICACAO)",
          "regex": "\\b(INIC1|SENT|DEC|PET|CERT|INT|CONC)\\b",
          "example": "INIC1, SENT, DEC",
          "location": "header",
          "_note": "Estes codigos sao IRRELEVANTES para classificacao semantica"
        }
      ]
    },
    "ESAJ": {
      "name": "Sistema de Automacao da Justica",
      "provider": "TJSP",
      "artifacts": [
        {
          "id": "esaj_document_code",
          "description": "Selo lateral vertical ESAJ",
          "regex": "c[óo]digo\\s+do\\s+documento:\\s*[A-Z0-9]{8,20}",
          "example": "Código do documento: ABC123DEF456",
          "location": "margin"
        },
        {
          "id": "esaj_digital_conference",
          "description": "Conferencia de documento digital ESAJ",
          "regex": "confer[êe]ncia\\s+de\\s+documento\\s+digital.*?portal\\s+e-saj",
          "example": "Conferência de documento digital no portal e-SAJ",
          "location": "footer"
        },
        {
          "id": "esaj_qr_url",
          "description": "QR Code ESAJ com URL",
          "regex": "https?://[a-z0-9.-]*\\.jus\\.br/[^\\s]*esaj[^\\s]*/documento[^\\s]*",
          "example": "https://esaj.tjsp.jus.br/documento/ABC123",
          "location": "footer"
        },
        {
          "id": "esaj_signature_bar",
          "description": "Barra de assinatura digital ESAJ",
          "regex": "assinado\\s+digitalmente\\s+por:\\s*[^\\n]{5,80}\\s+data:\\s*\\d{2}/\\d{2}/\\d{4}\\s+\\d{2}:\\d{2}:\\d{2}",
          "example": "Assinado digitalmente por: MARIA SANTOS Data: 25/11/2025 14:30:00",
          "location": "footer"
        },
        {
          "id": "esaj_tjsp_header",
          "description": "Brasao e logotipo TJSP",
          "regex": "tribunal\\s+de\\s+justi[çc]a\\s+do\\s+estado\\s+de\\s+s[ãa]o\\s+paulo\\s*-\\s*tjsp",
          "example": "TRIBUNAL DE JUSTICA DO ESTADO DE SAO PAULO - TJSP",
          "location": "header"
        },
        {
          "id": "esaj_resolution_552",
          "description": "Referencia Resolucao 552/11",
          "regex": "resolu[çc][ãa]o\\s+n?[º°]?\\s*552/11",
          "example": "Resolução nº 552/11",
          "location": "footer"
        },
        {
          "id": "esaj_watermark",
          "description": "Marca d'agua ESAJ",
          "regex": "\\[marca\\s+d.?[áa]gua:?\\s*esaj\\]",
          "example": "[marca d'agua ESAJ]",
          "location": "background"
        },
        {
          "id": "esaj_page_ref",
          "description": "Referencia de folhas ESAJ",
          "regex": "fls\\.\\s*\\d+(/\\d+)?",
          "example": "fls. 123",
          "location": "header",
          "_note": "Util para navegacao, NAO para classificacao"
        }
      ]
    },
    "EPROC": {
      "name": "Sistema de Processo Eletronico",
      "provider": "TRFs",
      "artifacts": [
        {
          "id": "eproc_p7s_ref",
          "description": "Referencia a arquivo .p7s (assinatura destacada)",
          "regex": "assinatura\\s+digital\\s+dispon[ií]vel\\s+em:\\s*[^\\n]*\\.p7s",
          "example": "Assinatura digital disponível em: documento.p7s",
          "location": "footer"
        },
        {
          "id": "eproc_iti_verifier",
          "description": "Verificador de Conformidade ITI",
          "regex": "verificador\\s+de\\s+conformidade\\s+(iti|icp-brasil)",
          "example": "Verificador de Conformidade ITI",
          "location": "footer"
        },
        {
          "id": "eproc_pades_seal",
          "description": "Selo PAdES padrao EPROC",
          "regex": "assinado\\s+eletronicamente\\s+por.*?certificado\\s+digital\\s+icp-brasil",
          "example": "Assinado eletronicamente por JOAO SILVA, certificado digital ICP-Brasil",
          "location": "footer"
        },
        {
          "id": "eproc_url",
          "description": "URL EPROC",
          "regex": "https?://[a-z0-9.-]*\\.(trf|tj)[a-z0-9]*\\.jus\\.br/[^\\s]*eproc[^\\s]*",
          "example": "https://eproc.trf4.jus.br/validar/ABC123",
          "location": "footer"
        },
        {
          "id": "eproc_byterange",
          "description": "ByteRange e referencias tecnicas CAdES",
          "regex": "byterange\\s*\\[\\s*\\d+\\s+\\d+\\s+\\d+\\s+\\d+\\s*\\]",
          "example": "ByteRange [0 1234 5678 9012]",
          "location": "footer"
        },
        {
          "id": "eproc_movement_ref",
          "description": "Referencia de movimentacao EPROC",
          "regex": "mov\\.\\s*\\d+",
          "example": "Mov. 123",
          "location": "header",
          "_note": "Util para navegacao, NAO para classificacao"
        }
      ]
    },
    "PROJUDI": {
      "name": "Processo Judicial Digital",
      "provider": "TJs Regionais",
      "artifacts": [
        {
          "id": "projudi_pades",
          "description": "Selo PAdES padrao PROJUDI",
          "regex": "digitalmente\\s+assinado\\s+por.*?data:\\s*\\d{2}/\\d{2}/\\d{4}",
          "example": "Digitalmente assinado por MARIA SILVA Data: 25/11/2025",
          "location": "footer"
        },
        {
          "id": "projudi_url",
          "description": "URL PROJUDI",
          "regex": "https?://[a-z0-9.-]*\\.jus\\.br/[^\\s]*projudi[^\\s]*",
          "example": "https://projudi.tjpr.jus.br/validar/ABC123",
          "location": "footer"
        },
        {
          "id": "projudi_assinador_tjrj",
          "description": "Assinador Livre TJRJ",
          "regex": "assinador\\s+livre\\s+(tjrj|tribunal\\s+de\\s+justi[çc]a\\s+do\\s+rio\\s+de\\s+janeiro)",
          "example": "Assinador Livre TJRJ",
          "location": "footer"
        },
        {
          "id": "projudi_version",
          "description": "Versao PROJUDI",
          "regex": "projudi\\s+-\\s+vers[ãa]o\\s+\\d+\\.\\d+(\\.\\d+)?",
          "example": "PROJUDI - Versão 3.2.1",
          "location": "footer"
        },
        {
          "id": "projudi_coat",
          "description": "Brasoes variados regionais",
          "regex": "\\[bras[ãa]o:?\\s*[^\\]]{5,50}\\]",
          "example": "[brasão TJPR]",
          "location": "header"
        }
      ]
    },
    "STF": {
      "name": "Supremo Tribunal Federal",
      "provider": "STF",
      "artifacts": [
        {
          "id": "stf_cpf_watermark",
          "description": "Marca d'agua com CPF do consulente STF",
          "regex": "cpf\\s+do\\s+consulente:\\s*\\d{3}\\.\\d{3}\\.\\d{3}-\\d{2}",
          "example": "CPF do consulente: 123.456.789-00",
          "location": "watermark"
        },
        {
          "id": "stf_watermark_alert",
          "description": "Alerta de marca d'agua STF",
          "regex": "a\\s+inser[çc][ãa]o\\s+da\\s+marca\\s+d.?[áa]gua\\s+se\\s+sobrescreve.*?sistemas\\s+internos\\s+do\\s+tribunal",
          "example": "A inserção da marca d'água se sobrescreve nos sistemas internos do Tribunal",
          "location": "footer"
        },
        {
          "id": "stf_pkcs7",
          "description": "Assinatura PKCS7 STF",
          "regex": "assinatura\\s+digital\\s+pkcs\\s*[#]?\\s*7",
          "example": "Assinatura Digital PKCS#7",
          "location": "footer"
        },
        {
          "id": "stf_validation_url",
          "description": "URL validacao STF",
          "regex": "https?://(www\\.)?stf\\.jus\\.br/[^\\s]*(validar|autenticar)[^\\s]*",
          "example": "https://stf.jus.br/validar/ABC123",
          "location": "footer"
        },
        {
          "id": "stf_victor",
          "description": "Projeto Victor (OCR automatica)",
          "regex": "documento\\s+processado\\s+pelo\\s+projeto\\s+victor",
          "example": "Documento processado pelo Projeto Victor",
          "location": "footer"
        },
        {
          "id": "stf_resolution_693",
          "description": "Resolucao STF 693/2020",
          "regex": "resolu[çc][ãa]o\\s+stf\\s+n?[º°]?\\s*693/2020",
          "example": "Resolução STF nº 693/2020",
          "location": "footer"
        }
      ]
    },
    "STJ": {
      "name": "Superior Tribunal de Justica",
      "provider": "STJ",
      "artifacts": [
        {
          "id": "stj_verification_code",
          "description": "Codigo de verificacao STJ",
          "regex": "c[óo]digo:\\s*[A-Z0-9]{16,32}",
          "example": "Código: ABCDEF1234567890GHIJ",
          "location": "footer"
        },
        {
          "id": "stj_auth_url",
          "description": "URL autenticacao STJ",
          "regex": "autentique\\s+em:\\s*https?://(www\\.)?stj\\.jus\\.br/validar[^\\s]*",
          "example": "Autentique em: https://stj.jus.br/validar/ABC123",
          "location": "footer"
        },
        {
          "id": "stj_certificate_data",
          "description": "Dados de certificado STJ",
          "regex": "assinado\\s+por:\\s*[^\\n]{5,80}\\s+-\\s+cpf:\\s*\\d{3}\\.\\d{3}\\.\\d{3}-\\d{2}",
          "example": "Assinado por: JOAO SILVA - CPF: 123.456.789-00",
          "location": "footer"
        },
        {
          "id": "stj_timestamp",
          "description": "Timestamp STJ",
          "regex": "data:\\s*\\d{2}/\\d{2}/\\d{4}\\s+\\d{2}:\\d{2}:\\d{2}\\s+(brt|brst|-03:?00|-02:?00)",
          "example": "Data: 25/11/2025 14:30:00 BRT",
          "location": "footer"
        },
        {
          "id": "stj_mp_disclaimer",
          "description": "Disclaimer MP 2.200-2/2001",
          "regex": "documento\\s+assinado\\s+digitalmente\\s+conforme\\s+mp\\s+n?[º°]?\\s*2\\.?200-2/2001",
          "example": "Documento assinado digitalmente conforme MP nº 2.200-2/2001",
          "location": "footer"
        },
        {
          "id": "stj_qr_instruction",
          "description": "Instrucao QR Code STJ",
          "regex": "valide\\s+este\\s+documento\\s+via\\s+qr\\s+code",
          "example": "Valide este documento via QR Code",
          "location": "footer"
        }
      ]
    },
    "UNIVERSAL": {
      "name": "Padroes Universais",
      "provider": "ICP-Brasil / Todos",
      "artifacts": [
        {
          "id": "universal_serial_number",
          "description": "Serial number de certificado (hexadecimal)",
          "regex": "serial\\s+number:?\\s*[0-9A-F]{16,}",
          "example": "Serial Number: 1234567890ABCDEF",
          "location": "footer"
        },
        {
          "id": "universal_sha1",
          "description": "Hash SHA-1",
          "regex": "sha-?1:?\\s*[0-9A-F]{40}",
          "example": "SHA1: ABCDEF1234567890ABCDEF1234567890ABCDEF12",
          "location": "footer"
        },
        {
          "id": "universal_sha256",
          "description": "Hash SHA-256",
          "regex": "sha-?256:?\\s*[0-9A-F]{64}",
          "example": "SHA256: (64 hex chars)",
          "location": "footer"
        },
        {
          "id": "universal_icp_ca",
          "description": "Autoridade Certificadora ICP-Brasil",
          "regex": "ac\\s+[a-z]+\\s+-\\s+icp-brasil",
          "example": "AC VALID - ICP-Brasil",
          "location": "footer"
        },
        {
          "id": "universal_page_number",
          "description": "Paginas numeradas isoladas",
          "regex": "^p[áa]gina\\s+\\d+\\s+(de|/)\\s+\\d+\\s*$",
          "example": "Página 1 de 10",
          "location": "footer"
        },
        {
          "id": "universal_separator_lines",
          "description": "Linhas separadoras esteticas",
          "regex": "^[_\\-=*]{10,}$",
          "example": "____________________",
          "location": "any"
        }
      ]
    }
  }
}
